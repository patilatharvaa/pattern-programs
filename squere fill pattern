import java.util.*;

class Main {
    public static void main(String[] args) {
        // squere fill pattern
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for (int i = 1; i <= n; i++) {
            for (int index = 1; index <= n; index++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
